{
  "@type": "Schema",
  "@context": "https://rocketschema.org/context",
  "name": "Customer",
  "description": "A person or organization that purchases goods or services from a business. Supports both B2C (individual consumers) and B2B (business customers) with financial tracking, loyalty programs, and segmentation capabilities.",
  "properties": [
    {
      "name": "customerNumber",
      "type": "string",
      "mode": "stored",
      "description": "Unique customer identifier or account number assigned by the business",
      "required": true,
      "example": "CUST-2024-00123"
    },
    {
      "name": "customerType",
      "type": "string",
      "mode": "enum",
      "enum": [
        "individual",
        "business"
      ],
      "description": "Type of customer - 'individual' for B2C (person), 'business' for B2B (organization)",
      "required": true,
      "example": "individual"
    },
    {
      "name": "person",
      "type": "Person",
      "mode": "stored",
      "description": "Reference to Person entity for B2C customers (when customerType is 'individual')"
    },
    {
      "name": "organization",
      "type": "Organization",
      "mode": "stored",
      "description": "Reference to Organization entity for B2B customers (when customerType is 'business')"
    },
    {
      "name": "status",
      "type": "string",
      "mode": "enum",
      "enum": [
        "active",
        "inactive",
        "suspended",
        "pending",
        "blacklisted"
      ],
      "description": "Current customer account status (active, inactive, suspended, pending, blacklisted)",
      "required": true,
      "example": "active"
    },
    {
      "name": "registrationDate",
      "type": "Date",
      "description": "Date when the customer account was created",
      "required": true,
      "example": "2024-01-15",
      "mode": "stored"
    },
    {
      "name": "lastModifiedDate",
      "type": "DateTime",
      "description": "Date and time when the customer record was last modified",
      "example": "2024-11-10T14:30:00Z",
      "mode": "stored"
    },
    {
      "name": "isGuest",
      "type": "boolean",
      "description": "Whether this is a guest checkout customer (no account registration required for purchase). Found in 30-60% of e-commerce transactions",
      "example": false,
      "mode": "stored"
    },
    {
      "name": "guestConvertedAt",
      "type": "DateTime",
      "description": "Date and time when a guest customer was converted to a registered customer account",
      "mode": "stored"
    },
    {
      "name": "firstPurchaseDate",
      "type": "Date",
      "description": "Date of the customer's first purchase",
      "example": "2024-01-20",
      "mode": "computed"
    },
    {
      "name": "lastPurchaseDate",
      "type": "Date",
      "description": "Date of the customer's most recent purchase",
      "example": "2024-10-15",
      "mode": "computed"
    },
    {
      "name": "totalOrders",
      "type": "number",
      "description": "Total number of orders placed by the customer",
      "example": 47,
      "mode": "computed"
    },
    {
      "name": "totalRevenue",
      "type": "MonetaryAmount",
      "description": "Total lifetime revenue from this customer",
      "mode": "computed"
    },
    {
      "name": "averageOrderValue",
      "type": "MonetaryAmount",
      "description": "Average value per order",
      "mode": "computed"
    },
    {
      "name": "preferredCurrency",
      "type": "Currency",
      "description": "Customer's preferred currency for pricing and invoices",
      "mode": "stored"
    },
    {
      "name": "timezone",
      "type": "string",
      "description": "Customer's timezone in IANA format (e.g., 'America/New_York', 'Europe/Paris') for scheduling and time-sensitive communications",
      "example": "America/New_York",
      "mode": "stored"
    },
    {
      "name": "creditLimit",
      "type": "MonetaryAmount",
      "description": "Maximum credit allowed for B2B customers (null for B2C or unlimited credit)",
      "mode": "stored"
    },
    {
      "name": "currentBalance",
      "type": "MonetaryAmount",
      "description": "Current outstanding balance owed by the customer",
      "mode": "computed"
    },
    {
      "name": "paymentTerms",
      "type": "PaymentTerm",
      "description": "Default payment terms for B2B customers (Net 30, Net 60, COD, etc.)",
      "mode": "stored"
    },
    {
      "name": "taxExempt",
      "type": "boolean",
      "description": "Whether the customer is exempt from sales tax",
      "example": false,
      "mode": "stored"
    },
    {
      "name": "taxExemptionCertificate",
      "type": "string",
      "description": "Tax exemption certificate number if applicable",
      "example": "TAX-EXEMPT-12345",
      "mode": "stored"
    },
    {
      "name": "loyaltyTier",
      "type": "string",
      "description": "Customer loyalty program tier (Bronze, Silver, Gold, Platinum, VIP)",
      "example": "Gold",
      "mode": "enum",
      "enum": [
        "Bronze",
        "Silver",
        "Gold",
        "Platinum",
        "VIP"
      ]
    },
    {
      "name": "loyaltyPoints",
      "type": "number",
      "description": "Current loyalty points balance",
      "example": 2450,
      "mode": "computed"
    },
    {
      "name": "loyaltyMemberSince",
      "type": "Date",
      "description": "Date when customer joined the loyalty program",
      "example": "2024-01-15",
      "mode": "stored"
    },
    {
      "name": "customerSegment",
      "type": "string",
      "description": "Marketing or business segment classification (VIP, Regular, New, Dormant, At-Risk, High-Value, etc.)",
      "example": "High-Value",
      "mode": "enum",
      "enum": [
        "VIP",
        "Regular",
        "New",
        "Dormant",
        "At-Risk",
        "High-Value",
        "Enterprise"
      ]
    },
    {
      "name": "referralCode",
      "type": "string",
      "description": "Unique referral code for this customer to share with others",
      "example": "REF-SMITH-2024",
      "mode": "stored"
    },
    {
      "name": "referredBy",
      "type": "Customer",
      "description": "Reference to the customer who referred this customer",
      "mode": "stored"
    },
    {
      "name": "marketingConsents",
      "type": "CustomerMarketingConsent[]",
      "description": "Marketing communication consent tracking per channel (email, sms, phone, mail, push) with timestamps and IP addresses for GDPR compliance",
      "mode": "stored"
    },
    {
      "name": "newsletter",
      "type": "object",
      "description": "Newsletter subscription tracking (separate from general marketing consent)",
      "properties": {
        "subscribed": "boolean",
        "subscribedAt": "DateTime",
        "subscribedIp": "string",
        "unsubscribedAt": "DateTime",
        "unsubscribeToken": "string"
      },
      "example": {
        "subscribed": true,
        "subscribedAt": "2024-01-15T10:30:00Z",
        "subscribedIp": "192.168.1.100",
        "unsubscribeToken": "a1b2c3d4e5f6g7h8"
      },
      "mode": "stored"
    },
    {
      "name": "gdprConsents",
      "type": "CustomerGdprConsent[]",
      "description": "GDPR data processing consent and rights tracking with version history for privacy policy, terms of service, and data processing agreements",
      "mode": "stored"
    },
    {
      "name": "communicationPreferences",
      "type": "object",
      "description": "Preferred communication channels and frequency",
      "example": {
        "channel": "email",
        "frequency": "weekly",
        "timezone": "America/New_York"
      },
      "mode": "stored"
    },
    {
      "name": "salesRepresentative",
      "type": "Employee",
      "description": "Assigned sales representative for B2B customers",
      "mode": "stored"
    },
    {
      "name": "accountManager",
      "type": "Employee",
      "description": "Assigned account manager for B2B customers",
      "mode": "stored"
    },
    {
      "name": "pricingTier",
      "type": "string",
      "description": "Pricing tier for special customer pricing (Standard, Volume, Wholesale, Partner)",
      "example": "Volume",
      "mode": "enum",
      "enum": [
        "Standard",
        "Volume",
        "Wholesale",
        "Partner"
      ]
    },
    {
      "name": "discountPercentage",
      "type": "number",
      "description": "Default discount percentage applied to orders (0-100)",
      "example": 15,
      "mode": "stored"
    },
    {
      "name": "annualRevenueTier",
      "type": "string",
      "description": "Annual revenue tier for B2B segmentation (0-1M, 1M-10M, 10M-100M, 100M+)",
      "example": "1M-10M",
      "mode": "enum",
      "enum": [
        "0-1M",
        "1M-10M",
        "10M-100M",
        "100M+"
      ]
    },
    {
      "name": "notes",
      "type": "string",
      "description": "Internal notes about the customer, preferences, or special requirements",
      "example": "Prefers morning deliveries. Requires invoice by email only.",
      "mode": "stored"
    },
    {
      "name": "tags",
      "type": "CustomerTag[]",
      "description": "Freeform tags for customer categorization and filtering",
      "example": [
        "wholesale",
        "priority",
        "tech-industry"
      ],
      "mode": "stored"
    },
    {
      "name": "parentCustomer",
      "type": "Customer",
      "description": "Reference to parent customer in corporate hierarchy for B2B subsidiary relationships",
      "mode": "stored"
    },
    {
      "name": "hierarchyLevel",
      "type": "number",
      "description": "Depth level in customer hierarchy (0 = root/top-level customer, 1 = direct subsidiary, 2+ = nested subsidiaries)",
      "example": 2,
      "mode": "computed"
    },
    {
      "name": "createdBy",
      "type": "User",
      "description": "User who created this customer record",
      "mode": "stored"
    },
    {
      "name": "createdAt",
      "type": "DateTime",
      "description": "Timestamp when customer record was created",
      "required": true,
      "mode": "stored"
    },
    {
      "name": "modifiedBy",
      "type": "User",
      "description": "User who last modified this customer record",
      "mode": "stored"
    },
    {
      "name": "modifiedAt",
      "type": "DateTime",
      "description": "Timestamp of last modification",
      "mode": "stored"
    },
    {
      "name": "deleted",
      "type": "boolean",
      "description": "Soft delete flag for data retention compliance",
      "example": false,
      "mode": "computed"
    },
    {
      "name": "deletedBy",
      "type": "User",
      "description": "User who soft-deleted this customer record",
      "mode": "stored"
    },
    {
      "name": "deletedAt",
      "type": "DateTime",
      "description": "Timestamp when customer record was soft-deleted",
      "mode": "stored"
    },
    {
      "name": "deletionReason",
      "type": "string",
      "description": "Business reason for customer deletion",
      "mode": "stored"
    },
    {
      "name": "changeHistory",
      "type": "CustomerChangeHistory[]",
      "description": "Comprehensive change log tracking all field-level modifications with before/after values for audit trail and regulatory compliance",
      "mode": "stored"
    },
    {
      "name": "externalReferences",
      "type": "CustomerExternalReference[]",
      "description": "References to customer records in external systems (Salesforce, SAP, etc.) for integration purposes",
      "example": [
        {
          "system": "Salesforce",
          "externalId": "0031234567890ABC",
          "syncedAt": "2024-11-10T10:00:00Z"
        },
        {
          "system": "SAP",
          "externalId": "1000123",
          "syncedAt": "2024-11-10T10:05:00Z"
        }
      ],
      "mode": "stored"
    },
    {
      "name": "dataQuality",
      "type": "object",
      "description": "Data quality metrics and duplicate management",
      "properties": {
        "qualityScore": "number",
        "lastVerifiedAt": "DateTime",
        "isDuplicate": "boolean",
        "masterRecordId": "string",
        "mergedIntoId": "string",
        "mergedAt": "DateTime"
      },
      "example": {
        "qualityScore": 95,
        "lastVerifiedAt": "2024-11-01T00:00:00Z",
        "isDuplicate": false
      },
      "mode": "stored"
    },
    {
      "name": "metadata",
      "type": "object",
      "description": "Additional metadata for extensibility and custom fields",
      "mode": "stored"
    }
  ],
