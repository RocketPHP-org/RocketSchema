{
  "@type": "Schema",
  "@context": "https://rocketschema.org/context",
  "name": "Gender",
  "description": "Gender identity reference data",
  "properties": [
    {
      "name": "code",
      "type": "string",
      "mode": "enum",
      "description": "Gender code",
      "enum": ["M", "F", "O", "U"],
      "required": true,
      "example": "F"
    },
    {
      "name": "label",
      "type": "string",
      "mode": "stored",
      "description": "Human-readable label",
      "required": true,
      "example": "Female"
    },
    {
      "name": "description",
      "type": "string",
      "mode": "stored",
      "description": "Detailed description"
    }
  ],
  "examples": [
    {
      "@type": "Gender",
      "code": "M",
      "label": "Male"
    },
    {
      "@type": "Gender",
      "code": "F",
      "label": "Female"
    },
    {
      "@type": "Gender",
      "code": "O",
      "label": "Other"
    },
    {
      "@type": "Gender",
      "code": "U",
      "label": "Unknown / Prefer not to say"
    }
  ]
}
