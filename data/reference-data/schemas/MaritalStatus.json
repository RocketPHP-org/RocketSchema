{
  "@type": "Schema",
  "@context": "https://rocketschema.org/context",
  "name": "MaritalStatus",
  "description": "Marital status reference data",
  "properties": [
    {
      "name": "code",
      "type": "string",
      "mode": "enum",
      "description": "Marital status code",
      "enum": ["S", "M", "D", "W", "P", "U"],
      "required": true,
      "example": "M"
    },
    {
      "name": "label",
      "type": "string",
      "mode": "stored",
      "description": "Human-readable label",
      "required": true,
      "example": "Married"
    },
    {
      "name": "description",
      "type": "string",
      "mode": "stored",
      "description": "Detailed description"
    }
  ],
  "examples": [
    {
      "@type": "MaritalStatus",
      "code": "S",
      "label": "Single"
    },
    {
      "@type": "MaritalStatus",
      "code": "M",
      "label": "Married"
    },
    {
      "@type": "MaritalStatus",
      "code": "D",
      "label": "Divorced"
    },
    {
      "@type": "MaritalStatus",
      "code": "W",
      "label": "Widowed"
    },
    {
      "@type": "MaritalStatus",
      "code": "P",
      "label": "Domestic Partnership / Civil Union"
    },
    {
      "@type": "MaritalStatus",
      "code": "U",
      "label": "Unknown / Prefer not to say"
    }
  ]
}
