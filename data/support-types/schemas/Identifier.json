{
  "@type": "Schema",
  "@context": "https://rocketschema.org/context",
  "name": "Identifier",
  "description": "A unique identifier issued by a specific system - tax IDs, registration numbers, business IDs, etc. Supports all country-specific identification systems",
  "properties": [
    {
      "name": "system",
      "type": "string",
      "description": "Identification system or authority that issued this identifier (e.g., 'SIREN', 'SIRET', 'VAT', 'EIN', 'DUNS', 'LEI')",
      "required": true,
      "example": "SIREN"
    },
    {
      "name": "value",
      "type": "string",
      "description": "The actual identifier value",
      "required": true,
      "example": "123456789"
    },
    {
      "name": "type",
      "type": "string",
      "description": "Type or category of identifier (e.g., 'tax', 'registration', 'legal', 'financial')",
      "example": "registration"
    },
    {
      "name": "country",
      "type": "string",
      "description": "Country code (ISO 3166-1 alpha-2) where this identifier is valid",
      "format": "iso-country-code",
      "example": "FR"
    },
    {
      "name": "issuer",
      "type": "string",
      "description": "Organization or authority that issued the identifier",
      "example": "INSEE France"
    },
    {
      "name": "label",
      "type": "string",
      "description": "Human-readable label for this identifier",
      "example": "French Business Registration Number"
    },
    {
      "name": "period",
      "type": "Period",
      "description": "Period during which this identifier is valid"
    },
    {
      "name": "use",
      "type": "string",
      "description": "Purpose of this identifier (official, temp, secondary)",
      "example": "official"
    }
  ],
  "examples": [
    {
      "@type": "Identifier",
      "system": "SIREN",
      "value": "123456789",
      "type": "registration",
      "country": "FR",
      "issuer": "INSEE France",
      "label": "French Business Registration Number",
      "use": "official"
    },
    {
      "@type": "Identifier",
      "system": "SIRET",
      "value": "12345678900012",
      "type": "registration",
      "country": "FR",
      "label": "French Establishment Number"
    },
    {
      "@type": "Identifier",
      "system": "VAT",
      "value": "FR12345678901",
      "type": "tax",
      "country": "FR",
      "label": "EU VAT Number"
    },
    {
      "@type": "Identifier",
      "system": "EIN",
      "value": "12-3456789",
      "type": "tax",
      "country": "US",
      "issuer": "IRS",
      "label": "US Employer Identification Number"
    },
    {
      "@type": "Identifier",
      "system": "DUNS",
      "value": "123456789",
      "type": "financial",
      "issuer": "Dun & Bradstreet",
      "label": "D-U-N-S Number"
    },
    {
      "@type": "Identifier",
      "system": "LEI",
      "value": "123456789012ABCDEFGH12",
      "type": "financial",
      "label": "Legal Entity Identifier"
    }
  ]
}
