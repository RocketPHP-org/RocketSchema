{
  "@type": "Schema",
  "@context": "https://rocketschema.org/context",
  "name": "QuantitativeValue",
  "description": "A quantitative value with unit of measurement",
  "properties": [
    {
      "name": "value",
      "type": "number",
      "description": "Numeric value",
      "required": true,
      "example": 2.5
    },
    {
      "name": "unitCode",
      "type": "string",
      "description": "Unit of measurement (UN/CEFACT code)",
      "required": true,
      "example": "KGM"
    },
    {
      "name": "unitText",
      "type": "string",
      "description": "Human-readable unit text",
      "example": "kilograms"
    },
    {
      "name": "minValue",
      "type": "number",
      "description": "Minimum value in range",
      "example": 1.0
    },
    {
      "name": "maxValue",
      "type": "number",
      "description": "Maximum value in range",
      "example": 5.0
    }
  ],
  "examples": [
    {
      "@type": "QuantitativeValue",
      "value": 2.5,
      "unitCode": "KGM",
      "unitText": "kilograms"
    },
    {
      "@type": "QuantitativeValue",
      "value": 15.6,
      "unitCode": "INH",
      "unitText": "inches"
    },
    {
      "@type": "QuantitativeValue",
      "minValue": 1.0,
      "maxValue": 5.0,
      "unitCode": "KGM",
      "unitText": "kilograms"
    }
  ]
}
